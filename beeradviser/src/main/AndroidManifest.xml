<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tda.saipc.beeradviser.beeradviser" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".FindBeerActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ReceiveBeerOrder"
            android:label="@string/title_activity_receive_beer_order"
            android:parentActivityName=".FindBeerActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.tda.saipc.beeradviser.beeradviser.FindBeerActivity" />
        </activity>
    </application>

</manifest>
